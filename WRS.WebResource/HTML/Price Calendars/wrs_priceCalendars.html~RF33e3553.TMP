                            };
                            var litag = $("<a href='javascript:;' onclick='OnParkSelect(id);' id='" + results.value[i]["wrs_id@OData.Community.Display.V1.FormattedValue"] + "'>" + results.value[i]["wrs_name"] + "</a>");
                            $("#parktagid").append(litag);
                            globalParkArray.push(Park);
